<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>


<!-- Content for the Add page -->
<main class="flex-fill">
<div class="container">
    <div class="row">
        <div class="offset-md-3 col-md-6">
            
                <h1><%= title %></h1>

                <form class="form" method="POST">
                <div class="form-group">
                    <label for="nameTextField">Survey Name <%=survey.surveyName %></label>
                    
                </div>
                <br>

                <% for(let count = 0; count < survey.questions.length; count++) { %>

                    <div class="form-group">
                        <label> Question <%= count + 1%> </label><br>
                        <input type="radio" id="">agree
                        <input type="radio" id="">disagree
            

                    </div>
                    <br>

                <% } %>
            
                <br>
                <button class="btn btn-primary" type="submit">
                    <i class="fas fa-edit"></i> Submit</button>
                
                    <a href="/home" class="btn btn-warning">
                        <i class="fas fa-undo"></i> Cancel</a>
                        <br>

                </form>
                <br>
                <br>
                <br>
                <br>
            
        </div>

    </div>
</div>

</main>


